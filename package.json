{
  "name": "flowneer",
  "version": "0.6.1",
  "description": "Zero-dependency fluent flow builder for AI agents",
  "license": "MIT",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./plugins": "./dist/plugins/index.js",
    "./plugins/observability": "./dist/plugins/observability/index.js",
    "./plugins/resilience": "./dist/plugins/resilience/index.js",
    "./plugins/persistence": "./dist/plugins/persistence/index.js",
    "./plugins/llm": "./dist/plugins/llm/index.js",
    "./plugins/dev": "./dist/plugins/dev/index.js",
    "./plugins/messaging": "./dist/plugins/messaging/index.js",
    "./plugins/tools": "./dist/plugins/tools/index.js",
    "./plugins/agent": "./dist/plugins/agent/index.js",
    "./plugins/memory": "./dist/plugins/memory/index.js",
    "./plugins/output": "./dist/plugins/output/index.js",
    "./plugins/telemetry": "./dist/plugins/telemetry/index.js",
    "./plugins/eval": "./dist/plugins/eval/index.js",
    "./plugins/graph": "./dist/plugins/graph/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "flow",
    "agent",
    "llm",
    "pipeline",
    "fluent",
    "builder"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "bun test && bun run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dotenv": "17.3.1",
    "openai": "6.22.0",
    "tsup": "^8.5.1",
    "vitepress": "^2.0.0-alpha.16"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}